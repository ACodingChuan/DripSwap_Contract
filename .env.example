# Copy this file to .env or .env.<network> and fill in your secrets.
# NETWORK-specific files (e.g. .env.sepolia) will override the base .env.

# EOA private key used for broadcasting deployments (hex string, 0x-prefixed).
DEPLOYER_PK=

# RPC endpoint for the target network. For local anvil you can use http://127.0.0.1:8545
RPC_URL=

# AI 协作服务
OPENAI_API_KEY=your_openai_api_key_here
GITHUB_TOKEN=your_github_token_here  
COPILOT_API_KEY=your_copilot_api_key_here
ETHERSCAN_API_KEY=your_etherscan_api_key_here
ALCHEMY_API_KEY=your_alchemy_api_key_here